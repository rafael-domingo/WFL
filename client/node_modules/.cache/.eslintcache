animate={{\n                            rotate: [180, 0],\n                            opacity: [0, 1]\n                        }}>\n                            <img src={back}/>                        \n                        </motion.div>\n                    </Link>\n                        <div style={{display: 'flex', justifyContent: 'center', alignItems: 'flex-start', flexWrap: 'wrap'}}>\n                        <motion.div\n                            initial = \"initial\"\n                            animate=\"enter\"\n                            transition={{ duration: 0.5 }}\n                            exit=\"exit\"\n                            variants={variants}\n                            className=\"components-motion\">\n                                <div className=\"components-restaurantdetail\">\n                                    <Description description=\"popular\" />\n                                    <img src={detail.image_url} style={{ width: '100%'}}/>\n                                    <Rating rating={detail.rating} />\n                                    <Price price={detail.price} />\n                                    <RestaurantInfo name={detail.name} address={detail.location.display_address} phone={detail.phone} />\n                                    <Yelp link={detail.url} />\n                                </div>\n                            </motion.div>\n                            <motion.div\n                            initial = \"initial\"\n                            animate=\"enter\"\n                            transition={{ duration: 0.5 }}\n                            exit=\"exit\"\n                            variants={variants}\n                            className=\"components-motion\">\n                                <div className=\"components-restaurantdetail\">\n                                    <Description description=\"cheap\" />\n                                    <img src={cheap.image_url} style={{ width: '100%'}}/>\n                                    <Rating rating={cheap.rating} />\n                                    <Price price={cheap.price} />\n                                    <RestaurantInfo name={cheap.name} address={cheap.location.display_address} phone={cheap.phone} />\n                                    <Yelp link={cheap.url} />\n                                </div>\n                            </motion.div>\n                            <motion.div\n                            initial = \"initial\"\n                            animate=\"enter\"\n                            transition={{ duration: 0.5 }}\n                            exit=\"exit\"\n                            variants={variants}\n                            className=\"components-motion\">\n                                <div className=\"components-restaurantdetail\">\n                                    <Description description=\"highly rated\" />\n                                    <img src={high.image_url} style={{ width: '100%'}}/>\n                                    <Rating rating={high.rating} />\n                                    <Price price={high.price} />\n                                    <RestaurantInfo name={high.name} address={high.location.display_address} phone={high.phone} />\n                                    <Yelp link={high.url} />\n                                </div>\n                            </motion.div>\n                        </div>\n               \n                </div>\n                \n            </AnimatePresence>\n        )\n    } else {\n        return (\n            <div style={{width: '100%', height: '100%', display: 'flex', justifyContent: 'center', alignItems: 'center'}}>\n                <ReactLoading type={'spin'} height={'10%'} width={'10%'} />\n            </div>\n        )\n    }\n \n}\n\nexport default RestaurantDetail;","/Users/rafaeldomingo/Documents/GitHub/WFL/client/src/containers/CusineList/index.js",["78"],"import './cuisineList.css';\n\nimport Header from '../../containers/Header';\nimport CuisineCard from '../../components/CuisineCard';\n\nimport { useParams, Link } from \"react-router-dom\";\n\nfunction CuisineList({cuisine}) {\n\n    return (\n        <div>\n            <div className=\"Cuisines\">\n            <Header text=\"What's For Lunch?\"/>\n                {\n                cuisine.cuisine.map((item, index) => {\n                    return (\n                        <div className=\"card\" key={item}>\n                            <Link to={`/${item}`} style={{textDecoration: 'none', color: 'white'}}>\n                                <CuisineCard cuisine={item}/>\n                            </Link>\n                        </div>\n                      \n                        \n                    )\n                })\n                }\n            </div>\n        </div>\n    )\n}\n\nexport default CuisineList;\n\n\n","/Users/rafaeldomingo/Documents/GitHub/WFL/client/src/elements/Rating/index.js",["79","80","81","82"],"import './rating.css';\n\nimport fill from '../../assets/business/star-filled.png';\nimport half from '../../assets/business/star-half-fill.png';\nimport empty from '../../assets/business/star-empty.png';\n\nfunction Rating({rating}) {\n    var numFill = Math.floor(rating);\n    if (rating - numFill > 0) {\n        var halfFill = true;\n    }\n    var emptyFill = 5 - Math.ceil(rating);\n    var starArray = [];\n    for (var i = 0; i < numFill ; i++) {\n        starArray.push(<img src={fill} key={i} style={{width: '20%'}}/>);\n    };\n    \n    if(halfFill) {\n        starArray.push(<img src={half} key={'halfstar'} style={{width: '20%'}}/>);\n    };\n\n    for (var i = 0; i < emptyFill; i++) {\n        starArray.push(<img src={empty} key={i+10} style={{width: '20%'}}/>);\n    }\n\n    return (\n        <div style={{width: '60%', margin: '20px'}}>\n            {starArray}\n        </div>\n    )\n}\n\nexport default Rating;","/Users/rafaeldo